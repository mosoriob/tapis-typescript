    "@tapis/tapis-typescript-tenants": "file:gen/tenants",
    "@tapis/tapis-typescript-tokens": "file:gen/tokens"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.2.4",
    "@types/chai": "^4.2.16",
    "@types/jest": "^26.0.22",
    "chai": "^4.3.4",
    "cross-fetch": "^3.1.4",
    "dotenv": "^8.2.0",
    "js-yaml": "^4.0.0",
    "mocha": "^8.3.2",
    "ts-node": "^9.1.1",
    "typescript": "~3.8.0"
  }
